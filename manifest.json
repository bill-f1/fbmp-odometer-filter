{
    "name": "Facebook Marketplace Mileage Filter",
    "version": "0.1",
    "manifest_version": 3,
    "description": "Adds a mileage filter for vehicles to Facebook marketplace.",
    "content_scripts": [
        {
            "matches": ["https://www.facebook.com/marketplace/*"],
            "css": ["mileage-filter.css"],
            "js": ["getFirstChild.js", "interface.js", "filter.js"]
        }
    ]
}